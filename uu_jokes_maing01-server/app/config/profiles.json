{
  "{asid}": {
    "profileList": ["AsidAuthorities","AsidExecutives","AsidLicenseOwner","Public"],
    "useCaseMap": {}
  },
  "*": {
    "profileList": ["Authorities","Executives","AwidLicenseOwner","Public"],
    "useCaseMap": {
      "sys/uuAppWorkspace/init": {
        "sysStateList": ["created"],
        "profileList": ["AwidLicenseOwner"]
      },
      "defaultUve": {
        "profileList": ["Public"]
      },
      "sys/uuAppWorkspace/initUve": {
        "sysStateList": ["created"],
        "profileList": ["Public","AwidLicenseOwner"]
      },
      "jokesInstance/init": {
        "sysStateList": ["active"],
        "profileList": ["AwidLicenseOwner"]
      },
      "joke/create": {
        "sysStateList": ["active"],
        "profileList": ["Authorities","Executives"]
      },
      "joke/get": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "Readers"]
      },
      "joke/update": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives"]
      },
      "joke/updateVisibility": {
        "sysStateList": ["active"],
        "profileList": ["Authorities"]
      },
      "joke/delete": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives"]
      },
      "joke/list": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Executives", "Readers"]
      }
    }
  }
}
